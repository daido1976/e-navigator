<h1>Interview Edit</h1>
<%= form_for [@user, @interview] do |f| %>
  <div class="form-group">
    <%= f.label :interview_datetime %><br />
    <%= f.datetime_select :interview_datetime, use_month_numbers: :true, minute_step: 15 %>
  </div>
  <div>
    <%= f.submit "Update", class: "btn btn-primary" %>
  </div>
<% end %>
